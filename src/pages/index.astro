---
import Layout from '../layouts/Layout.astro';
import Navigation from '../components/Navigation.astro';
import Hero from '../components/Hero.astro';
import QuickWin from '../components/QuickWin.astro';
import IntegrationPaths from '../components/IntegrationPaths.astro';
import WhyOTEL from '../components/WhyOTEL.astro';
import Features from '../components/Features.astro';
import DeveloperTestimonials from '../components/DeveloperTestimonials.astro';
import Footer from '../components/Footer.astro';
---

<Layout>
  <Navigation />
  <main id="main-content">
    <Hero />
    <section id="quick-win">
      <QuickWin />
    </section>
    <section id="integration">
      <IntegrationPaths />
    </section>
    <section id="why-otel">
      <WhyOTEL />
    </section>
    <section id="features">
      <Features />
    </section>
    <section id="testimonials">
      <DeveloperTestimonials />
    </section>
  </main>
  <Footer />
</Layout>
